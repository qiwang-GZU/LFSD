{
  "name": "lfsd",
  "description": "LFSD: Self-Adaptive Prototype Decoupling for Few-Shot Object Detection",
  "version": "1.0.0",
  "exported_from": "mmdetectron conda environment",
  "environment": {
    "python": {
      "version": "3.9",
      "recommended": "3.9.23"
    },
    "cuda": {
      "version": "12.1",
      "driver": ">=525.60.13"
    }
  },
  "dependencies": {
    "pytorch": {
      "version": "2.2.2+cu121",
      "install": "pip install torch==2.2.2+cu121 torchvision==0.17.2+cu121 --extra-index-url https://download.pytorch.org/whl/cu121"
    },
    "torchvision": {
      "version": "0.17.2+cu121"
    },
    "detectron2": {
      "version": "0.3",
      "install": "python -m pip install detectron2 -f https://dl.fbaipublicfiles.com/detectron2/wheels/cu121/torch2.2/index.html",
      "note": "Version must match PyTorch and CUDA versions"
    },
    "mmcv-full": {
      "version": "1.7.2",
      "install": "pip install mmcv-full==1.7.2 -f https://download.openmmlab.com/mmcv/dist/cu121/torch2.2.0/index.html",
      "note": "Required for SAIA (Self-Adaptive Instance Attention) deformable attention module"
    },
    "mmdet": {
      "version": "2.28.2",
      "install": "pip install mmdet==2.28.2"
    }
  },
  "python_packages": {
    "numpy": "1.26.4",
    "scipy": "1.13.1",
    "opencv-python": "4.11.0.86",
    "pillow": "11.3.0",
    "matplotlib": "3.9.4",
    "tensorboard": "2.20.0",
    "fvcore": "0.1.5.post20221221",
    "pycocotools": "2.0.10",
    "cloudpickle": "3.1.1",
    "tabulate": "0.9.0",
    "termcolor": "3.1.0",
    "yacs": "0.1.8",
    "tqdm": "4.67.1",
    "scikit-learn": "1.6.1",
    "scikit-image": "0.24.0",
    "pandas": "2.3.3",
    "seaborn": "0.13.2",
    "timm": "1.0.22",
    "open-clip-torch": "2.24.0",
    "addict": "2.4.0",
    "iopath": "0.1.10",
    "shapely": "2.0.7",
    "imgaug": "0.4.0"
  },
  "system_requirements": {
    "os": "Linux",
    "gcc": ">=4.9",
    "gpu": {
      "type": "NVIDIA",
      "vram": ">=8GB",
      "recommended_vram": ">=16GB"
    }
  },
  "datasets": {
    "voc": {
      "VOC2007": {
        "size": "0.8GB",
        "url": "https://drive.google.com/file/d/1BcuJ9j9Mtymp56qGSOfYxlXN4uEVyxFm/view"
      },
      "VOC2012": {
        "size": "3.5GB",
        "url": "https://drive.google.com/file/d/1NjztPltqm-Z-pG94a6PiPVP4BgD8Sz1H/view"
      },
      "vocsplit": {
        "size": "<1MB",
        "url": "https://drive.google.com/file/d/1BpDDqJ0p-fQAFN_pthn2gqiK5nWGJ-1a/view"
      }
    },
    "coco": {
      "coco2014": {
        "size": "~19GB",
        "url": "https://cocodataset.org/#download"
      },
      "cocosplit": {
        "size": "174MB",
        "url": "https://drive.google.com/file/d/1T_cYLxNqYlbnFNJt8IVvT7ZkWb5c0esj/view"
      }
    }
  },
  "pretrained_weights": {
    "resnet101_imagenet": {
      "detectron2_format": {
        "url": "https://drive.google.com/file/d/1rsE20_fSkYeIhFaNU04rBfEDkMENLibj/view",
        "filename": "R-101.pkl"
      },
      "pytorch_format": {
        "url": "https://download.pytorch.org/models/resnet101-5d3b4d8f.pth",
        "filename": "resnet101-5d3b4d8f.pth"
      }
    }
  },
  "config": {
    "saia": {
      "MODEL.BACKBONE.WITHSAIA": {
        "type": "bool",
        "default": false,
        "description": "Enable SAIA (Self-Adaptive Instance Attention) module"
      },
      "MODEL.BACKBONE.SAIA_ALPHA": {
        "type": "float",
        "default": 0.3,
        "range": [0.0, 1.0],
        "description": "Fusion weight for SAIA enhanced features"
      },
      "MODEL.BACKBONE.FREEZE_SAIA": {
        "type": "bool",
        "default": false,
        "description": "Freeze SAIA module parameters during training"
      },
      "MODEL.ROI_HEADS.FREEZE_FEAT_WITH_SAIA": {
        "type": "bool",
        "default": true,
        "description": "Freeze ROI feature extraction when SAIA is enabled"
      }
    },
    "prototype_memory": {
      "MODEL.ROI_HEADS.MEMORY": {
        "type": "bool",
        "default": false,
        "description": "Enable prototype memory bank"
      },
      "MODEL.ROI_HEADS.QUEUE_LEN": {
        "type": "int",
        "default": 2048,
        "description": "Length of prototype memory queue per class"
      }
    },
    "dual_stream": {
      "DATASETS.TWO_STREAM": {
        "type": "bool",
        "default": false,
        "description": "Enable dual-stream training with orthogonal decoupling"
      },
      "MODEL.ROI_HEADS.SEMANTIC": {
        "type": "bool",
        "default": false,
        "description": "Enable semantic distillation loss"
      },
      "MODEL.ROI_HEADS.AUGMENTATION": {
        "type": "bool",
        "default": false,
        "description": "Enable feature augmentation from base prototypes"
      }
    }
  }
}
